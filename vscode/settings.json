{
	/**
	 * Typography
	 **/
	"editor.fontSize": 14,
	"editor.lineHeight": 1.7,
	"editor.fontLigatures": true,
	"editor.suggestFontSize": 12,
	// "editor.fontFamily": "SFMono Nerd Font",
	// "editor.fontFamily": "Fantasque Sans Mono",
	// "editor.fontFamily": "Geist Mono",
	// "editor.fontFamily": "Jetbrains Mono",
	"editor.fontFamily": "JetBrainsMono Nerd Font",
	// "editor.fontFamily": "FantasqueSansM Nerd Font Mono",
	// "editor.fontFamily": "M+1Code Nerd Font",
	// "editor.fontFamily": "Source Code Pro",
	// "editor.fontFamily": "Monaco",
	// "editor.fontFamily": "MesloLGS NF",
	// "editor.fontWeight": "400",
	// "editor.letterSpacing": 0.4,
	"editor.suggestLineHeight": 20,
	"terminal.integrated.fontSize": 14,
	"terminal.integrated.lineHeight": 1.7,
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
	"workbench.iconTheme": "file-icons-colourless",
	"workbench.list.smoothScrolling": true,
	// "workbench.fontAliasing": "antialiased",

	"workbench.preferredDarkColorTheme": "GitHub Dark Colorblind (Beta)",
	"workbench.preferredLightColorTheme": "GitHub Light Colorblind (Beta)",
	"editor.copyWithSyntaxHighlighting": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.emptySelectionClipboard": true,
	"workbench.editor.enablePreview": false,
	"window.newWindowDimensions": "inherit",
	"editor.multiCursorModifier": "ctrlCmd",
	"diffEditor.renderSideBySide": false,
	"files.trimTrailingWhitespace": true,
	"editor.accessibilitySupport": "off",
	"editor.snippetSuggestions": "top",
	"workbench.startupEditor": "none",
	"editor.detectIndentation": false,
	// "window.nativeFullScreen": true,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"editor.minimap.enabled": false,
	"editor.lineNumbers": "on",
	"files.exclude": {
		"vendor/": true,
		".git/": true,
		".vscode/": true,
		".idea/": true,
		".fleet/": true,
		"**/.git": true,
		"**/node_modules/": true,
		"node_modules/": true,
		"**/.next/": false,
		".next/": false,
		"**/.nuxt/": false,
		".nuxt/": false,
		"build/": true
	},

	/*
	 * Misc
	 */
	"editor.wordWrap": "on",
	"workbench.tree.indent": 22,
	"editor.cursorStyle": "block",
	"editor.cursorWidth": 3,
	"explorer.compactFolders": true,
	"workbench.settings.editor": "json",
	"workbench.sideBar.location": "left",
	"workbench.activityBar.location": "hidden",
	"editor.inlineSuggest.enabled": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit"
	},
	"git.confirmSync": false,
	"typescript.updateImportsOnFileMove.enabled": "always",

	/**
	 * Silence The Noise
	 */
	"breadcrumbs.enabled": true,
	"editor.hover.enabled": true,
	"editor.hover.sticky": true,
	"scm.diffDecorations": "none",
	"editor.colorDecorators": true,
	"workbench.tips.enabled": false,
	// "editor.matchBrackets": "always",
	"git.decorations.enabled": false,
	// "editor.lightbulb.enabled": false,
	"editor.selectionHighlight": false,
	"editor.overviewRulerBorder": false,
	"editor.renderLineHighlight": "all",
	"editor.occurrencesHighlight": "off",
	// "problems.decorations.enabled": false,
	// "editor.renderControlCharacters": false,
	"editor.hideCursorInOverviewRuler": true,
	"workbench.editor.showTabs": "multiple",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleDeclarations": "goto",
	// "workbench.statusBar.visible": false,
	// "workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",

	/**
	 * Code
	 **/
	// Include "-" in word selection.
	"editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
	"emmet.triggerExpansionOnTab": true, // enable tab to expanse emmet tags
	"laravel-pint.enable": true,
	"emmet.includeLanguages": {
		"vue-html": "html",
		"vue": "html",
		"javascript": "javascriptreact",
		"typescript": "typescriptreact",
		// "javascriptreact": "html",
		// "typescriptreact": "html",
		"react-native": "html"
	},
	"files.associations": {
		"*.mdx": "mdx",
		"*.css": "css",
		"*.view.php": "php",
		"*.blade.php": "blade"
	},
	"blade.format.enable": true, // if you would like to enable blade format
	"[blade]": {
		"editor.autoClosingBrackets": "always",
		"editor.defaultFormatter": "shufo.vscode-blade-formatter",
		"editor.formatOnSave": true,
		"editor.tabSize": 4
	},

	/**
	 * Find
	 **/
	"search.useIgnoreFiles": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/_ide_helper.php": true,
		"**/composer.lock": true,
		"**/package-lock.json": true,
		"**/yarn.json": true,
		"**/.next": true,
		"**/.nuxt": true,
		"**/dist": true
	},

	/**
	 * PHP Intelephense
	 **/
	"php.suggest.basic": true,

	// /**
	//  * PHP CS Fixer
	//  **/
	// "[php]": {
	//   "editor.defaultFormatter": "junstyle.php-cs-fixer",
	//   "editor.tabSize": 4
